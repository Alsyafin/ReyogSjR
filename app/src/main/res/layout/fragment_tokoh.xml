<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Bottom.TokohFragment">

    <FrameLayout

        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">


        <RelativeLayout
            android:id="@+id/layout_top"
            android:layout_width="match_parent"
            android:layout_height="200dp">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <com.example.windows.reyogsjr._sliders.SliderView
                    android:id="@+id/sliderView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>

            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bg_indicator"/>

            <LinearLayout

                android:id="@+id/pagesContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_gravity="bottom"
                android:layout_marginBottom="8dp"
                android:gravity="center_horizontal"
                android:orientation="horizontal"/>

        </RelativeLayout>
        <LinearLayout

            android:orientation="horizontal"
            android:layout_marginTop="207dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:padding="5dp"
                android:paddingRight="5dp"
                android:paddingLeft="5dp"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:id="@+id/warok"
                        android:padding="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:id="@+id/imageView0"
                            android:background="@color/colorPrimary"
                            android:layout_marginRight="15dp"
                            android:layout_width="50dp"
                            android:layout_height="50dp" />
                        <TextView
                            android:layout_toRightOf="@+id/imageView0"
                            android:text="Warok"
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    </RelativeLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>
            <LinearLayout
                android:padding="5dp"
                android:paddingRight="5dp"
                android:paddingLeft="5dp"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:id="@+id/jathil"
                        android:padding="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:id="@+id/imageView"
                            android:background="@color/colorPrimary"
                            android:layout_marginRight="15dp"
                            android:layout_width="50dp"
                            android:layout_height="50dp" />
                        <TextView
                            android:layout_toRightOf="@+id/imageView"
                            android:text="Jathil"
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    </RelativeLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>



        </LinearLayout>

        <LinearLayout

            android:orientation="horizontal"
            android:layout_marginTop="307dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:padding="5dp"
                android:paddingRight="5dp"
                android:paddingLeft="5dp"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:id="@+id/ganong"
                        android:padding="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:id="@+id/imageView2"
                            android:background="@color/colorPrimary"
                            android:layout_marginRight="15dp"
                            android:layout_width="50dp"
                            android:layout_height="50dp" />
                        <TextView
                            android:layout_toRightOf="@+id/imageView2"
                            android:text="Ganongan"
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    </RelativeLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>
            <LinearLayout
                android:padding="5dp"
                android:paddingRight="5dp"
                android:paddingLeft="5dp"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:id="@+id/kelono"
                        android:padding="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:id="@+id/imageView3"
                            android:background="@color/colorPrimary"
                            android:layout_marginRight="15dp"
                            android:layout_width="50dp"
                            android:layout_height="50dp" />
                        <TextView
                            android:layout_toRightOf="@+id/imageView3"
                            android:text="Kelono"
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    </RelativeLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>



        </LinearLayout>
        <LinearLayout

            android:orientation="horizontal"
            android:layout_marginTop="407dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:padding="5dp"
                android:paddingRight="5dp"
                android:paddingLeft="5dp"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:id="@+id/dadak"
                        android:padding="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:id="@+id/imageView5"
                            android:background="@color/colorPrimary"
                            android:layout_marginRight="15dp"
                            android:layout_width="50dp"
                            android:layout_height="50dp" />
                        <TextView
                            android:layout_toRightOf="@+id/imageView5"
                            android:text="Dadak"
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    </RelativeLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>
            <LinearLayout
                android:padding="5dp"
                android:paddingRight="5dp"
                android:paddingLeft="5dp"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:id="@+id/pengrawit"
                        android:padding="15dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:id="@+id/imageView6"
                            android:background="@color/colorPrimary"
                            android:layout_marginRight="15dp"
                            android:layout_width="50dp"
                            android:layout_height="50dp" />
                        <TextView
                            android:layout_toRightOf="@+id/imageView6"
                            android:text="Pengrawit"
                            android:layout_centerVertical="true"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    </RelativeLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</LinearLayout>
